#!/usr/bin/env bash
source /dev/stdin < <(cli shim)
. $(dirname ${BASH_SOURCE})/../cli.sh

main() {
        
    local path='.'
    for i in "$@"; do
        path="${path}/${i}"
    done

    (
        cd "$path"
        for entry in *; do 
            [ -d "$entry" ] && [ -x "$entry" ] && echo $entry
        done
    )
}

shim "$@"
