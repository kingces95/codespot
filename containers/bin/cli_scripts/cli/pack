#!/usr/bin/env bash
source /dev/stdin < <(cli shim)

help() {
    cat << EOF
Command

Summary
    
Description

Arguments

Global Arguments
    --help -h       : Show this message and exit.
    --self-test     : Runs a self test.
EOF
}

main() {
    return
}

self_test() (
    return
)

cli::shim "$@"